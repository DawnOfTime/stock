<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="goodsmanagement" namespace="/goodsmanagement" extends="struts-default">
		<!-- 展示树 -->
		<action name="tree" class="com.stock.goodsmanagement.action.GoodsmanagementAction" method="tree"></action>
		<!-- 增改 -->	
		<action name="save" class="com.stock.goodsmanagement.action.GoodsmanagementAction" method="save"></action>
		<!-- 删 -->
		<action name="removetree" class="com.stock.goodsmanagement.action.GoodsmanagementAction" method="removetree"></action>
	</package>
</struts>